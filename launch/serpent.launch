<launch>
	<param name="robot_description" 
	

	textfile="$(find serpent_gazebo)/urdf/serpent.urdf" />
	<node name="urdf_spawner" pkg="gazebo_ros" type="spawn_model" respawn="false" output="screen" args="-urdf -model serpent -param robot_description -y -6"/>

<!-- converts joint states to TF transforms -->
<node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">

    <remap from="/joint_states" to="/serpent_gazebo/joint_states" />
</node>
</launch>
